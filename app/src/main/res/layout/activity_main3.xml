                tools:layout_editor_absoluteX="123dp"
                tools:layout_editor_absoluteY="-6dp" />

            <TextView
                android:id="@+id/title4"
                android:layout_width="205dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="Wroking Title #01"
                android:textSize="18sp" />

            <ImageButton
                android:id="@+id/share4"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/share"
                android:scaleType="fitCenter" />

        </LinearLayout>

        <Space
            android:layout_width="match_parent"
            android:layout_height="29dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/xplr05"
                android:layout_width="51dp"
                android:layout_height="51dp"
                android:layout_weight="1"
                android:onClick="xplrfive"
                app:srcCompat="@mipmap/xplrlogo"
                tools:layout_editor_absoluteX="123dp"
                tools:layout_editor_absoluteY="-6dp" />

            <TextView
                android:id="@+id/title5"
                android:layout_width="212dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="Wroking Title #01"
                android:textSize="18sp" />

            <ImageButton
                android:id="@+id/share5"
                android:layout_width="50dp"
                android:layout_height="match_parent"
                android:background="@drawable/share"
                android:scaleType="fitCenter" />

        </LinearLayout>

        <Space
            android:layout_width="match_parent"
            android:layout_height="32dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/xplr06"
                android:layout_width="51dp"
                android:layout_height="51dp"
                android:layout_weight="1"
                android:onClick="xplrsix"
                app:srcCompat="@mipmap/xplrlogo"
                tools:layout_editor_absoluteX="123dp"
                tools:layout_editor_absoluteY="-6dp" />

            <TextView
                android:id="@+id/title6"
                android:layout_width="218dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="Wroking Title #01"
                android:textSize="18sp" />

            <ImageButton
                android:id="@+id/share6"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/share"
                android:scaleType="fitCenter" />

        </LinearLayout>

    </LinearLayout>

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="218dp"
        android:layout_height="35dp"
        android:layout_marginEnd="60dp"
        android:layout_marginRight="60dp"
        android:clickable="true"
        android:onClick="roux"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/rouxstudios" />

</androidx.constraintlayout.widget.ConstraintLayout>