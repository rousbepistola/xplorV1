        android:layout_height="484dp"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider">

        <Space
            android:layout_width="match_parent"
            android:layout_height="24dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="49dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/xplr01"
                android:layout_width="51dp"
                android:layout_height="51dp"
                android:layout_weight="1"
                android:onClick="xplrone"
                app:srcCompat="@mipmap/xplrlogo"
                tools:layout_editor_absoluteX="123dp"
                tools:layout_editor_absoluteY="-6dp" />

            <TextView
                android:id="@+id/title1"
                android:layout_width="198dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="Wroking Title #01"
                android:textSize="18sp" />

            <ImageButton
                android:id="@+id/share1"
                android:layout_width="50dp"
                android:layout_height="match_parent"
                android:background="@drawable/share"
                android:scaleType="fitCenter" />

        </LinearLayout>

        <Space
            android:layout_width="match_parent"
            android:layout_height="29dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="51dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/xplr02"
                android:layout_width="51dp"
                android:layout_height="51dp"
                android:layout_weight="1"
                android:onClick="xplrtwo"
                app:srcCompat="@mipmap/xplrlogo"
                tools:layout_editor_absoluteX="123dp"
                tools:layout_editor_absoluteY="-6dp" />

            <TextView
                android:id="@+id/title2"
                android:layout_width="203dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="Wroking Title #02"
                android:textSize="18sp" />

            <ImageButton
                android:id="@+id/share2"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/share"
                android:scaleType="fitCenter" />

        </LinearLayout>

        <Space
            android:layout_width="match_parent"
            android:layout_height="29dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="51dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/xplr03"
                android:layout_width="51dp"
                android:layout_height="51dp"
                android:layout_weight="1"
                android:onClick="xplrthree"
                app:srcCompat="@mipmap/xplrlogo"
                tools:layout_editor_absoluteX="123dp"
                tools:layout_editor_absoluteY="-6dp" />

            <TextView
                android:id="@+id/title3"
                android:layout_width="203dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="Wroking Title #01"
                android:textSize="18sp" />

            <ImageButton
                android:id="@+id/share3"
                android:layout_width="50dp"
                android:layout_height="match_parent"
                android:background="@drawable/share"
                android:scaleType="fitCenter" />

        </LinearLayout>

        <Space
            android:layout_width="match_parent"
            android:layout_height="32dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="51dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/xplr04"
                android:layout_width="51dp"
                android:layout_height="51dp"
                android:layout_weight="1"
                android:onClick="xplrfour"
